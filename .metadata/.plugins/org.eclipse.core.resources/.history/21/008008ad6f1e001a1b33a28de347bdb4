package chengji;
import java.util.*;

public class chengji {
	
	
	public void input(){
		Scanner sc = new Scanner(System.in);

		Map mp = new HashMap();
		String a="";
		double b;
		while(!a.equals("noname")) {
			a = sc.next();
			b = sc.nextDouble();
			mp.put(a,b);
		}
		a = sc.next();
		double ans = (double) mp.get(a);
		System.out.printf("%.2lf\n",ans*0.21);
	}
	public static void main(String []args) {
		chengji a = new chengji();
		a.input();
	}
}
